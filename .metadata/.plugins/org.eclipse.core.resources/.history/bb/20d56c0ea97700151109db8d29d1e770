	public static void TestAdminDAO(){
		Cineplex cineplex = new AdminDAO().getCineplexByUserNameAndPassword("admin1", "password", "THE CATHAY CINEPLEX");
		System.out.println("Method : \ngetCineplexByUserNameAndPassword : returns cineplex");
		System.out.println("Output : \ncompanyName:"+cineplex.getCompanyName()+", \naddress:"+cineplex.getAddress()+", \nname:"+cineplex.getName()+", \ncinema size:"+cineplex.getCinemas().size()+", \nmovie size:"+cineplex.getMovies().size());
		System.out.println(cineplex.getCinemas().get(0).getCinemaCode());
	}
