package model;

import java.util.ArrayList;

public class Cinema {
	private String cinemaCode;
	private CINEMACLASS cinemaClass;
	private ArrayList[][]  seats;
	
	public Cinema(String cinemaCode, CINEMACLASS cinemaClass, ArrayList[][]seats){
		setCinemaCode(cinemaCode);
		setCinemaClass(cinemaClass);
		setSeats(seats);
	}
	
	public String getCinemaCode() {
		return cinemaCode;
	}

	public void setCinemaCode(String cinemaCode) {
		this.cinemaCode = cinemaCode;
	}

	public CINEMACLASS getCinemaClass() {
		return cinemaClass;
	}

	public void setCinemaClass(CINEMACLASS cinemaClass) {
		this.cinemaClass = cinemaClass;
	}

	public ArrayList[][] getSeats() {
		return seats;
	}

	public void setSeats(ArrayList[][] seats) {
		this.seats = seats;
	}
	
}
